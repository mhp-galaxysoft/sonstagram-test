<ul class="channel-list">
    <% for(let i = 0; i < livechnl.length; i++){ %>
        <li>
            <a class="channel-list-link" href="/channel/main/<%= livechnl[i].chnl_pk %>/live">
                <div class="channel-list-img">
                    <img src="<%= livechnl[i].mem_img %>">
                </div>
                <div class="channel-list-name">
                    <h3><%= livechnl[i].mem_nickname %></h3>
                    <% if( livechnl[i].factory_img ) { %>
                    <div class="channel-list-factorylogo">
                        <img src="<%= livechnl[i].factory_img %>">
                    </div>
                    <% } %>
                </div>
            </a>
            <div class="channel-list-txt">
                <p><%= livechnl[i].chnl_info %></p>
            </div>
            <div class="channel-list-btn">
                <% if(livechnl[i].f_yn === 0){ %>
                    <button class="user-unBtn" id="follow-btn" onclick="following(<%= livechnl[i].chnl_pk %>); this.onclick=null;">팔로잉</button>
                <% }else { %>
                    <button class="user-unBtn-un" id="unfollow-btn" onclick="unfollowing(<%= livechnl[i].chnl_pk %>); this.onclick=null;">팔로잉 취소</button>
                <% } %>
                <a href="/channel/main/<%= livechnl[i].chnl_pk %>/live" class="user-chBtn m-ch-chBtn">채널 구경가기</a>
            </div>
        </li>
    <% } %>
</ul>
<div style="width:100%; margin: 50px 0;">
    <nav>
        <ul class="pagination justify-content-center">
            <% if(yn_prev === 1) { %>
                <li class="page-item"><a class="page-link" href="javascript:void(0);" onclick="getList(<%= parseInt(pageNo)-1 %>)">이전</a></li>
            <% } %>
            <% for(let page = startPage; page <= endPage; page++) { %>
                <% if(page == pageNo) { %>
                    <li class="page-item active"><a class="page-link" href="javascript:void(0);" onclick="getList(<%= page %>)"><%= page %></a></li>
                <% } else { %>
                    <li class="page-item"><a class="page-link" href="javascript:void(0);" onclick="getList(<%= page %>)"><%= page %></a></li>
                <% } %>
            <% } %>
            <% if(yn_next === 1) { %>
                <li class="page-item"><a class="page-link" href="javascript:void(0);" onclick="getList(<%= parseInt(pageNo)+1 %>)">다음</a></li>
            <% } %>
        </ul>
    </nav>
</div>