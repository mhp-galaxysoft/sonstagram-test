<ul class="factory-channel-list">
    <% for(let i = 0; i < channels.length; i++){ %>
    <li>
        <div class="factory-channel-img">
            <img src="<%= channels[i].mem_img %>">
        </div>
        <div class="factory-channel-name">
            <h3><%= channels[i].mem_nickname %></h3>
            <% if(channels[i].factory_img) { %>
            <div class="factory-channel-factorylogo">
                <img src="<%= channels[i].factory_img %>">
            </div>
            <% } %>
        </div>
        <div class="factory-channel-txt">
            <p><%= channels[i].chnl_info %></p>
        </div>
        <div class="factory-channel-btn">
            <a href="/channel/main/<%= channels[i].chnl_pk %>/live" class="user-chBtn">채널 구경가기</a>

        </div>
        <div class="m-fc-btn">
            <a href="/channel/main/<%= channels[i].chnl_pk %>/live" class="m-user-chBtn">
                <img src="/images/mobile/icon-visit.svg" alt="채널 방문하기">
            </a>
        </div>
    </li>
    <% } %>
</ul>
<div style="width:100%; margin: 0; background: #fff; padding: 0 0 0 0;">
    <nav>
        <ul class="pagination justify-content-center">
            <% if(yn_prev === 1) { %>
                <li class="page-item"><a class="page-link" href="javascript:void(0);" onclick="getList(<%= parseInt(pageNo)-1 %>)">이전</a></li>
            <% } %>
            <% for(let page = startPage; page <= endPage; page++) { %>
                <% if(page == pageNo) { %>
                    <li class="page-item active"><a class="page-link" href="javascript:void(0);" onclick="getList(<%= page %>)"><%= page %></a></li>
                <% } else { %>
                    <li class="page-item"><a class="page-link" href="javascript:void(0);" onclick="getList(<%= page %>)"><%= page %></a></li>
                <% } %>
            <% } %>
            <% if(yn_next === 1) { %>
                <li class="page-item"><a class="page-link" href="javascript:void(0);" onclick="getList(<%= parseInt(pageNo)+1 %>)">다음</a></li>
            <% } %>
        </ul>
    </nav>
</div>

<!-- 스크롤 스타일 -->
<style>
    .factory-channel-list::-webkit-scrollbar {
        width: 7px;
    }

    .factory-channel-list::-webkit-scrollbar-track {
        background: none;
    }

    .factory-channel-list::-webkit-scrollbar-thumb {
        background: #FFBABA;
    }
</style>
