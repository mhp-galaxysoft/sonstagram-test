<div class="user-follow">
    <ul class="user-fw-list">
        <% for( let i = 0; i < followChnl.length; i++ ) {%>
        <li>
            <div class="user-fw-img">
                <img src="<%= followChnl[i].mem_img %>">
            </div>
            <div class="user-fw-name">
                <h3><%= followChnl[i].mem_nickname %></h3>
                <% if(followChnl[i].factory_img) { %>
                <div class="user-fw-factorylogo">
                     <img src="<%= followChnl[i].factory_img %>">
                </div>
                <% } %>
            </div>
            <div class="user-fw-txt">
                <p><%= followChnl[i].chnl_info %></p>
            </div>
            <div class="user-fw-btn">
                <button type="button" id="follow-btn<%=followChnl[i].chnl_pk%>" class="user-unBtn-un" onclick="unfollowing(<%= followChnl[i].chnl_pk %>)" >팔로잉 취소</button>
                <a href="/channel/main/<%= followChnl[i].chnl_pk %>/live" class="user-chBtn" >채널 구경가기</a>
            </div>
            <div class="m-fw-btn">
                <button type="button" id="m-follow-btn<%=followChnl[i].chnl_pk%>" class="m-user-unBtn" onclick="unfollowing(<%= followChnl[i].chnl_pk %>)" style="width: 98%;">
                    <img class="m-user-unBtn1" src="/images/mobile/icon-check.svg" alt="팔로우">
                </button>
                <a href="/channel/main/<%=followChnl[i].chnl_pk%>/live" class="m-user-chBtn" style="margin: 3px 0 0 0">
                    <img src="/images/mobile/icon-visit.svg" alt="채널 방문하기">
                </a>
            </div>
        </li>
        <% } %>
    </ul>
    <div class="pag-style">
        <nav>
            <ul class="pagination justify-content-center">
                <% if(yn_prev === 1) { %>
                    <li class="page-item"><a class="page-link" href="javascript:void(0);" onclick="getFollowList(<%= parseInt(pageNo)-1 %>)">이전</a></li>
                <% } %>
                <% for(let page = startPage; page <= endPage; page++) { %>
                    <% if(page == pageNo) { %>
                        <li class="page-item active"><a class="page-link" href="javascript:void(0);" onclick="getFollowList(<%= page %>)"><%= page %></a></li>
                    <% } else { %>
                        <li class="page-item"><a class="page-link" href="javascript:void(0);" onclick="getFollowList(<%= page %>)"><%= page %></a></li>
                    <% } %>
                <% } %>
                <% if(yn_next === 1) { %>
                    <li class="page-item"><a class="page-link" href="javascript:void(0);" onclick="getFollowList(<%= parseInt(pageNo)+1 %>)">다음</a></li>
                <% } %>
            </ul>
        </nav>
    </div>
</div>