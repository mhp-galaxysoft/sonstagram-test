<% for(let i= 0; i < lives.length; i++){ %>
    <div class="live-slide">
        <a href="/broadcast/live/<%= lives[i].live_pk %>">
            <div class="live-slide-img">
                <img src="<%= lives[i].live_thumbnail %>">
                <span class="live-span">현재 <%= lives[i].live_ccu %>명 시청중</span>
            </div>
            <h4 class="live-slide-tit"><%= lives[i].live_title %></h4>
            <div class="live-slide-ui">
            <span class="ui-pactory-logo">
                <img src="<%= lives[i].mem_img %>">
            </span>
                <h5><%= lives[i].mem_nickname %></h5>
                <%if(lives[i].factory_img) { %>
                    <span class="ui-pactory-logo">
                    <img src="<%= lives[i].factory_img %>">
                </span>
                <% } %>
            </div>
        </a>
    </div>
<% } %>
<% for(let i = 0; i < vods.length; i++){ %>
    <div class="live-slide">
        <a href="/broadcast/vod/<%= vods[i].vod_pk %>">
            <div class="live-slide-img">
                <img src="<%= vods[i].live_thumbnail %>">
                <span class="accrue-span"><%= vods[i].live_created%></span>
                <span class="time-span"><%= vods[i].vod_length %></span>
            </div>
            <h4 class="live-slide-tit"><%= vods[i].live_title %></h4>
            <div class="live-slide-ui">
                <div class="ui-img">
                    <img src="<%= vods[i].mem_img %>">
                </div>
                <h5><%= vods[i].mem_nickname %></h5>
                <% if(vods[i].factory_img) { %>
                <span class="ui-pactory-logo">
                <img src="<%= vods[i].factory_img %>">
                </span>
                <% } %>
            </div>
        </a>
    </div>
<% } %>
<div style="width:100%;">
    <nav>
        <ul class="pagination justify-content-center">
            <% if(yn_prev === 1) { %>
                <li class="page-item"><a class="page-link" href="javascript:void(0);" onclick="getList(<%= parseInt(pageNo)-1 %>)">이전</a></li>
            <% } %>
            <% for(let page = startPage; page <= endPage; page++) { %>
                <% if(page == pageNo) { %>
                    <li class="page-item active"><a class="page-link" href="javascript:void(0);" onclick="getList(<%= page %>)"><%= page %></a></li>
                <% } else { %>
                    <li class="page-item"><a class="page-link" href="javascript:void(0);" onclick="getList(<%= page %>)"><%= page %></a></li>
                <% } %>
            <% } %>
            <% if(yn_next === 1) { %>
                <li class="page-item"><a class="page-link" href="javascript:void(0);" onclick="getList(<%= parseInt(pageNo)+1 %>)">다음</a></li>
            <% } %>
        </ul>
    </nav>
</div>